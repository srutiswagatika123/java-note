//wap solve previous program using variable concept
class Test
{
	public static void main(String arg[])
	{
	   double db; //declartion
       boolean b;
       char ch;
       String s;
       db=5.0;   //initlization
       b=true;
       ch='y';
       s="ok";
	    System.out.println(db); //display
	     System.out.println(b);
	     System.out.println(ch);
	     System.out.println(s);
	  }
}


o/p:
5.0
true
y
ok



class Test
{
	public static void main(String arg[])
	{
	   double db=5.0; //declartion and initlization in one line
       boolean b=true;
       char ch='y';
       String s="ok";
       
	    System.out.println(db); //display
	     System.out.println(b);
	     System.out.println(ch);
	     System.out.println(s);
	  }
}

o/p:
5.0
true
y
ok


